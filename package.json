{

  "name": "react-flux-todoMVC-starter",
  "description": "react-flux and todoMVC-starter project",
  "repository":"https://github.com/Morgantheplant/react-starter-flux-todoMVC.git",
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify main.js | uglifyjs -cm > public/bundle.js",
    "start": "ecstatic -p 8000 public",
    "watch": "watchify main.js -o public/bundle.js -dv"
  },
  "dependencies": {
    "browserify": "^10.2.6",
    "ecstatic": "~0.8.0",
    "classnames": "^2.1.3",
    "ecstatic": "~0.8.0",
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "object-assign": "^4.0.1",
    "react": "~0.13.3",
    "reactify": "^1.1.1",
    "uglify": "~0.1.5",
    "watchify": "^3.2.3"
  },
  "license": "public domain"
}
